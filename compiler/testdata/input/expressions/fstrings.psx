view FStringExpressions(name: str, items: list, total: float):
    discount = 0.1
    
    <div>
        <h1>f"Welcome, {name}!"</h1>
        <p>f"You have {len(items)} items in your cart"</p>
        <p>f"Total: ${total:.2f}"</p>
        <p>f"Discount: {discount:.1%}"</p>
        <p>f"Final total: ${total * (1 - discount):.2f}"</p>
        
        for i, item in enumerate(items):
            <div>f"{i + 1}. {item}"</div>
            
        <div>
            f"Processing {'completed' if len(items) > 0 else 'pending'}"
        </div>
    </div>