view FStringExpressions(name: str, items: list, total: float):
    discount = 0.1

    <div>
        <h1>Welcome, {name}!</h1>
        <p>You have {len(items)} items in your cart</p>
        <p>Total: ${total:.2f}</p>
        <p>Discount: {discount:.1%}</p>
        <p>Final total: ${total * (1 - discount):.2f}</p>

        for i, item in enumerate(items):
            <div>{i + 1}. {item}</div>

        <div>
            Processing {'completed' if len(items) > 0 else 'pending'}
        </div>
    </div>